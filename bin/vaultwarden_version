#!/bin/bash

case $1 in
	bin)
		## Binary
                #old: cd ~/vaultwarden/output
                #old: ./vaultwarden --version | cut -d" " -f2
                docker exec vaultwarden /vaultwarden --version  | grep Vaultwarden  | cut -d" " -f2
	;;
	web)
		## Web Interface
                #old: cd ~/vaultwarden/output/web-vault
                #old: cat version.json | cut -d"\"" -f4
                docker exec vaultwarden /vaultwarden --version  | grep Web-Vault  | cut -d" " -f2
	;;
esac
